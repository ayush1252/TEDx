<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.ayush.tedxdtu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:debuggable="false"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.app.ayush.tedxdtu.SignInActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.app.ayush.tedxdtu.SpeakerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.app.ayush.tedxdtu.Profile"
            android:parentActivityName="com.app.ayush.tedxdtu.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppThemeNoAction" />
        <activity
            android:name="com.app.ayush.tedxdtu.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.app.ayush.tedxdtu.SpeakerRecyclerView"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.app.ayush.tedxdtu.RegistrationActivity"
            android:screenOrientation="portrait"
            android:theme="@style/MyMaterialTheme"
            android:windowSoftInputMode="stateHidden" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <service android:name="com.app.ayush.tedxdtu.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name="com.app.ayush.tedxdtu.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name="com.app.ayush.tedxdtu.NetworkServices.WebViewC" />
        <activity android:name="com.app.ayush.tedxdtu.AboutTheTheme"
            android:theme="@style/AppThemeNoAction"
            android:parentActivityName="com.app.ayush.tedxdtu.MainActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>